html {
    margin: 0 auto;
    padding: 0px;
    background: none;
}
body {
    margin: 0 auto;
    padding:0px;
    background:/*#0d4879*/#fff;
    font-family: Arial;
    font-size: 12px;
    line-height: 1.8em;
    color: #444;
    background: url("../images/login_bg01.png");
    position: relative;
}

.top{
	margin: 0px;
	padding: 0px;
	height: 200px;
	width: 100%;
	text-align: center;
	float: left;
}
.login_top{
	margin: 0 auto;
    padding:0px;
    width:562px;
    height: 74px;
    margin-top:64px;
    background: url("../images/login_top.png");
}
.center{
	margin: 0px;
	padding: 0px;
	height:100%;
	width:100%;
	text-align: center;
	float: left;
}
.center .login_box{
	position:absolute;
    width:402px;
    height: 461px;
    background: url("../images/login_box.png");
}
.login_form_top{
	margin: 0px;
	padding: 0px;
	width:225px;
	height: 68px;
	background: url("../images/logo_01.png");
	float: left;
	margin: 30px 89px 30px 88px;
}
.login_form_center{
	margin: 0px;
	padding: 0px;
	float: left;
	height: 220px;
	width: 100%;
}
.username_label{
	margin: 0px;
	padding: 0px;
	width:300px;
	height: 50px;
	background: url("../images/account.png");
	float: left;
	margin: 5px 51px 10px 51px;
}
.username,.password{
	margin: 0px;
	padding: 0px;
	width:220px;
	height: 50px;
	float: left;
	margin-left:60px;
	display: block;
	border: none;
	background: none;
	outline: none;
	list-style: none;
	line-height: 50px;
	font-size: 14px;
}
input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill{
	background-color: none;
} 
.password_label{
	margin: 0px;
	padding: 0px;
	width:300px;
	height: 50px;
	background: url("../images/pwd.png");
	float: left;
	margin: 5px 51px 10px 51px;
}
.login_btn{
	margin: 0px;
	padding: 0px;
	width:308px;
	height: 58px;
	display: block;
	border: none;
	outline: none;
	list-style: none;
	background: url("../images/login_btn.png");
	float: left;
	margin: 25px 47px 10px 47px;
	cursor: pointer;
}
.login_btn:hover{
	background: url("../images/login_btn_hover.png");
}
.validate_label{
	margin: 0px;
	padding: 0px;
	width:300px;
	height: 50px;
	background: url("../images/validate.png");
	float: left;
	margin: 5px 51px 10px 51px;
}
.code{
	margin: 0px;
	padding: 0px;
	width:100px;
	height: 50px;
	float: left;
	margin-left:55px;
	display: block;
	border: none;
	background: none;
	outline: none;
	list-style: none;
	line-height: 50px;
	font-size: 14px;
}
.code_val{
	margin: 0px;
	padding: 0px;
	width:100px;
	height: 50px;
	float: left;
	margin-left:15px;
	display: block;
	line-height: 50px;
	font-size: 14px;
	cursor: pointer;
}


.bottom{
	margin: 0px;
	padding: 0px;
	height: 100px;
	width: 100%;
	text-align: center;
	float: left;
}
.login_bottom{
	margin: 0px;
	padding: 0px;
    height:40px;
    width: 100%;
    margin-top: 30px;
    float: left;
}
